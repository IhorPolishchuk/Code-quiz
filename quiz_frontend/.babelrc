{
	"presets":["env","react","stage-2"],
	"plugins": [
		"transform-object-rest-spread",
		["transform-runtime", {
			"polyfill": false,
			"regenerator": true
		}],
		["import", {
			"libraryName": "antd", 
			"style": "css" 
		}]
	],
	"env": {
	    "development": {
	    	"presets":[
	    		"react-hmre"
	    	],
			"plugins": [
				"react-hot-loader/babel",
				["import", {
					"libraryName": "antd", 
					"style": "css" 
				}]
			]
		}
	}
}